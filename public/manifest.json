{
    "manifest_version": 3,
    "key": "",
    "name": "MQL Extensions",
    "description": "Extension for MyQuestionList which lets users add the url of current page directly from the extensions bar.",
    "version": "1.0.1",
    "action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "externally_connectable": {
        "matches": [
            "https://my-question-list.vercel.app/*"
        ]
    }
}